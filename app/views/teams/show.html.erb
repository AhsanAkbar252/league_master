                    <tr>
                      <td class="team-roster-table__number"><%= team_player.number %></td>
                      <td class="team-roster-table__position"><%= team_player.player.first_name %></td>
                      <td class="team-roster-table__name"><%= team_player.player.last_name %></td>
                      <td class="team-roster-table__name"><%= team_player.player.email %></td>
                      <td class="team-roster-table__name"><%= team_player.player.phone %></td>
                      <td class="team-roster-table__name"><%= link_to 'Delete', team_player, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-default btn-outline btn-xs card-header__button" %></td>
                    </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
